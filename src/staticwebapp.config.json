{
    "routes": [
      {
        "route": "/*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/account*",
        "rewrite": "/index.html"
      },
      {
        "route": "/profile.html",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/login",
        "rewrite": "/.auth/login/github"
      }
    ]
}